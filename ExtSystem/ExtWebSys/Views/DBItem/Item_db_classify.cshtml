                $.extend($,{'AddOrEidtNum@{@Classify_id;}@{@Num;}':2 })

                $(".Classify_h1@{@Classify_id;}@{@Num;}").html("修改数据");

                $("#btn_@{@DBName}@{@Classify_id;}@{@Num;}_op span").html("修改");
                edit_fun(id)


            }


