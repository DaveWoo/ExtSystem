                var rows= $.Cue_Classify_ListData["id_"+id];


                if(!rows)return;

                $.extend($,{"SelectClassify_ID{@Classify_id;}@{@Num;}":rows.role_menu})
                for(var rws in rows){


                    var obj=   $(".@{@DBName}@{@Classify_id;}@{@Num;}_input[name='"+rws+"']");
                    if(obj!=null&&obj.length>0){
                        var isRadio = (obj.attr("type")+ "").indexOf("radio") != -1;
                        if(isRadio){



                            for(var l=0;l<obj.length;l++){

                                if($(obj[l]).val().indexOf(rows[rws])!=-1){
                                    $(obj[l]).attr("checked","true")
                                }

                            }
                        }else
                        {

                            $(obj).val(rows[rws])

                        }


                    }

                }
            }

